# Survey Pro Frontend Environment Variables
# Copy this file to .env and update the values according to your setup

# API Configuration
# The base URL for your Laravel backend API
# For local development: http://localhost:8000/api
# For production: https://your-backend-domain.com/api
VITE_API_BASE_URL=http://localhost:8000/api

# Application Configuration
# The name of your application (displayed in browser title, etc.)
VITE_APP_NAME="Survey Pro"

# The base URL where your frontend is hosted
# For local development: http://localhost:5173
# For production: https://your-frontend-domain.com
VITE_APP_URL=http://localhost:5173

# Environment
# Set to 'development' for local development, 'production' for live deployment
VITE_NODE_ENV=development

# Optional: Analytics and Tracking
# Add your analytics tracking IDs if you use them
# VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
# VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Optional: External Services
# Add API keys for external services if you integrate them
# VITE_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxxxxxx
# VITE_PUSHER_APP_KEY=your_pusher_key
# VITE_PUSHER_CLUSTER=your_pusher_cluster

# Optional: Feature Flags
# Enable/disable certain features
# VITE_ENABLE_REGISTRATION=true
# VITE_ENABLE_SOCIAL_LOGIN=false
# VITE_ENABLE_DARK_MODE=true

# Optional: File Upload Configuration
# Maximum file size for uploads (in MB)
# VITE_MAX_FILE_SIZE=10

# Optional: Pagination
# Default number of items per page
# VITE_DEFAULT_PAGE_SIZE=20

# Optional: Session Configuration
# Token expiration time (in minutes)
# VITE_TOKEN_EXPIRY=1440

